#include <iostream>
#include <cmath>

using namespace std;

int main() {

	int tab[10][10]{};

	for (int i = 0; i < 10; i++) {
		for (int j = 0; j < 10; j++) {

			if (j == 0) {
				tab[i][j] = i;
			}
			else
			if (j == 1) {
				tab[i][j] = tab[i][j - 1] + tab[i][j - 1];
			}
			else
			if (j == 2) {
				tab[i][j] = pow(tab[i][j - 2], 2);
			}
			else
			if (j == 3) {
				tab[i][j] = (i) + (j);
				}
			else
			if (j == 4) {
				tab[i][j] = (i) - (j);
			}
			else {
				tab[i][j] = 0;
			}
		}
	}

	for (int i = 0; i < 10; i++) {
		for (int j = 0; j < 10; j++) {
			cout << tab[i][j] << " ";
		}
		cout << endl;
	}

	return 0;
}
